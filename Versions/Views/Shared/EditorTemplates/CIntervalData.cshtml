@using Household.MvcExtensions;

@model Household.BL.DATA.txx.Implementations.CIntervalData

@{
	string m_strBaseAction = Url.ActionInterval("");
}

<div class="w100">
	<div class="col-xs-12">
		@Html.LabelFor(m => m.Name)
		@Html.KnockoutTextBox("tbxName", "value: Name", true)
	</div>
</div>

@Scripts.Render("~/bundles/Interval")
<script type="text/javascript">
	(function () {
		Interval.Fill('@Html.Raw(m_strBaseAction)', @Model.ID, '@Html.Raw(HttpUtility.JavaScriptStringEncode(Model.Name))');
		MasterData.start();
		Interval.start();
	})();
</script>