@model Household.BL.DATA.txx.CIntervalData

<div class="row">
	<div class="col-xs-12">
		<h3>Interval</h3>
	</div>
	<div class="col-xs-12 mB10">
		<input type="button" class="button-default" id="btnSave" value="Save" onclick="Interval.Save();" />
		<input type="button" class="button-default" id="btnDelete" value="Delete" onclick="Interval.Delete();" />
		<input type="button" class="button-default" id="btnClose" value="Close" onclick="Common.hideSubContent();" />
	</div>
	<div class="col-xs-12">
		<ul class="" id="ulError"></ul>
	</div>
</div>
<form class="row">
	<div class="col-xs-12">
		<span>Name</span>
		<input type="text" id="tbxName" data-bind="value: Name" required />
	</div>
</form>

<script type="text/javascript">
	(function () {
		if(@Model.ID < 1){$('#btnDelete').remove();}
	})();
</script>